<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  	
  <servlet>
  	<servlet-name>FnolServlet</servlet-name>
  	<display-name>FnolServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.core.servlets.GetFnolData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>searching</servlet-name>
  	<display-name>searching</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.core.servlets.SearchFnol</servlet-class>
  </servlet>
  

  
  <servlet-mapping>
  	<servlet-name>FnolServlet</servlet-name>
  	<url-pattern>/fnols</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>searching</servlet-name>
  	<url-pattern>/searchfnol</url-pattern>
  </servlet-mapping>
 
  
  
</web-app>
